
<div id="movie-form">
<div class="movie-form">
<%= simple_form_for [@list, @bookmark] do |f| %>
<%= f.input :comment %>
<%= f.association :movie %>
<%= f.submit "Add Movie", class: "btn btn-success" %>
<button class="btn btn-warning"><%= link_to "Back", lists_path %></button>
<% end %>
</div>
</div>
